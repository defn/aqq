#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"

  aqq index "${BASH_SOURCE##*/aqq-}" '
    import "aws" as aws;

    .AutoScalingGroups
      | aws::index(.AutoScalingGroupARN) 
      + aws::index(.AutoScalingGroupName) 
    ' "$@"
}

source sub "$BASH_SOURCE" "$@"

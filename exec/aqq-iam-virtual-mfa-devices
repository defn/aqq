#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"

  aqq index "${BASH_SOURCE##*/aqq-}" '
    import "aws" as aws;

    .VirtualMFADevices
      | aws::index(.SerialNumber) 
      + aws::index(.User.Arn) 
      + aws::index(.User.UserId) 
      + aws::index(.User.UserName) 
    ' "$@"
}

source sub "$BASH_SOURCE" "$@"
